{
  "dependencies": {
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.1",
    "Ecommerce.Application": "1.0.0-*",
    "Ecommerce.Domain": "1.0.0-*",
    "Ecommerce.Infra.CrossCutting.Identity": "1.0.0-*",
    "Ecommerce.Infra.Data": "1.0.0-*",
    "Microsoft.NETCore.App": {
      "version": "1.0.1",
      "type": "platform"
    }
  },


  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
